{
  "@context":
  {
    "@base": "http://bkuczenski.github.io/lca-tools-datafiles/LCA/",
    "entity": "http://bkuczenski.github.io/lca-tools-datafiles/LCA/",
    "prop": "http://bkuczenski.github.io/lca-tools-datafiles/LCA/property/",
    "prov": "http://www.w3.org/ns/prov#",
    "dcterms": "http://purl.org/dcterms#",
    "qudt": "http://www.qudt.org/qudt/owl/1.0.0/qudt#",
    "lca": "http://descartes-core.org/ontologies/lca/1.0/LCAPattern.owl#",
    "stscope": "http://descartes-core.org/ontologies/lca/1.0/stscope.owl#",
    "olca": "http://greendelta.github.io/olca-schema/",
    
    "Name" : "rdfs:label",
    "Comment" : "rdfs:comment",

    "lcaCatalog":
    {
      "@id": "prop:lcaCatalog",
      "@type": "dc:title"
    },
    "dataSourceReference":
    {
      "@id": "prop:dataSourceReference",
      "@type": "prov:hadPrimarySource"
    },
    "dataSourceType": "prop:dataSourceType",
    
    "referenceEntity": "prop:referenceEntity",
    "entityId": "@id",
    "entityType": "prop:entityType",
    "origin": { "@id": "prop:origin", "@type": "dcterms:source"},
    "externalId": {"@id": "prop:externalId", "@type": "prov:wasDerivedFrom" },

    "flow": { "@id": "lca:Flow", "@type": "@id" },
    "flows": { "@id": "flow", "@container" : "@set"},

    "Compartment": { "@id": "lca:hasCompartment", "@container": "@list"},
    "CasNumber": { "@id": "prop:hasCasNumber", "@type": "olca:Flow/cas" },
    "characterizations": { "@id": "prop:Characterization", "@container": "@set" },
    "Formula": { "@id": "prop:hasFormula", "@type": "olca:Flow/formula" },

    "process": { "@id": "lca:Activity", "@type": "@id" },
    "processes": { "@id": "process", "@container" : "@set"},
    "exchanges": { "@id": "prop:Exchange", "@container": "@set" },
    "direction": "prop:Direction",

    "Classifications": { "@id": "prop:hasClassification", "@container": "@list"},
    "SpatialScope": { "@id": "stscope:scopedByPlace", "@type": "rdfs:place" },
    "TemporalScope": { "@id": "stscope:scopedByTime", "@container": "@index"},

    "quantity": { "@id": "entity:Quantity", "@type": "@id" },
    "quantities": { "@id": "quantity", "@container" : "@set"},
    
    "isReference": "prop:isReference",
    "referenceUnit": { "@id": "prop:hasUnit", "@type": "qudt:Unit"},
   
    "Indicator": { "@id": "prop:hasLciaIndicator", "@type": "prop:Indicator" },
    "Method": { "@id": "prop:isLciaMethod", "@type": "prop:LciaMethod" },
    "Category": { "@id": "prop:isInLciaImpactCategory", "@type": "pro:LciaImpactCategory" },

    "value": "prop:Value"

  }
}
